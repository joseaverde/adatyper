HEIGHT := 48
WIDTH := 169

AREA := HEIGHT * WIDTH        -- 8112 letters^2

GEN_TIME := 0.001986000 / 8   -- 0.24749975 s
REN_TIME := 1.979998000 / 8   -- 0.00024825 s
TIME := GEN_TIME + REN_TIME   -- 0.247748   s

LETTER_TIME := TIME / AREA    -- 3.0540927e-05 seconds/letter

LETTERS_FPS := AREA / TIME / 30  -- 1091.4316 letters => Surface.Area < 1091
